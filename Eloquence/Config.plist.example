    <dict>
        <key>BaseEndpoint</key>
        <string>https://YOUR_RESOURCE_NAME.cognitiveservices.azure.com</string>
        <key>APIKey</key>
        <string>YOUR_AZURE_API_KEY_HERE</string>
        <key>Whisper</key>
        <dict>
            <key>DeploymentName</key>
            <string>whisper</string>
            <key>APIVersion</key>
